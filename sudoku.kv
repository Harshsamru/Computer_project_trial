MDScreen:
    name:'sudoku'
    id: main_win
    orientation: "vertical"
    spacing: 10
    space_x: self.size[0]/3
    canvas.before:
        Color:
            rgba: (1, 1, 1, 1)
        Rectangle:
            source:"SUDOKU (7).png"
            size: root.width, root.height
            pos: self.pos
    MDFloatLayout:
        MDTextField:
            mode:'rectangle'
            max_text_length:1
            size_hint:0.05,0.09
            pos_hint:{'center_x':.3,'center_y':.8}
            multiline:False
            line_color_normal: "white"

